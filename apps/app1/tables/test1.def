
label My funny table  1

grant read insert to 999
grant insert update to 1;10

#no-copy
#no-preview

web.grid.fields_list  name  ctime   suma     ref.name

=NAME

  type char 1024

#  unique
  required

=CTIME

  type utime

=SUMA

  type real .4
  grant read
  web.grid.hide


=INDEX: TEST

  fields CTIME SUMA
  unique

=INDEX: TEST2

  fields NAME CTIME SUMA

=DES

  label [~descr]

  type char 128
  #password
  web.rows 5
  web.grid.maxlen 12
  web.preview.maxlen 12

=AMOUNT1

  type real .2
  required

=AMOUNT2

  type real

=AMOUNT3

  type real 10.5
  index
  unique

=AMOUNT4

  type real
  index
  unique
  #ro
  #read_only
  #system
  #web.hidden
  
=GRP
  type link DE_GROUPS NAME
  deny all to all
  grant all to 1
  label [~Group]

=TESTMAP

  type  backlink  testmap key_left

=REF

  type link test2 name

#  web.select-where    CNT > ?; 2
  web.combo            %40s=%-15s; NAME; CNT
  web.combo.orderby    NAME
#  web.edit.mono
  web.select_filter   MORE_CNT

#deny all
#grant cross 

=BACKREF

  type backlink test3 PARENT

=BOOL_TEST

  type bool
  required
  web.grid.editable
  web.view.editable

=DATE_TEST

    type  date
    web.view.details

=REPORTING

    type  file

=DO: DATE_TEST

  label   Test date switch
  grant   execute
  
  web.print


=ACTION: TEST

  label   Go to whatever
  target  IMPULSE
  grant   all

=ACTION: MYVIEW

  label   My view
  target  VIEW
  grant   all

=DO: POST_INSERT

  grant   execute
  
  web.hide
  web.print

=DO: POST_UPDATE

  grant   execute
  
  web.hide
  web.print
