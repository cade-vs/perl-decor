
label ToDo Notes
grant all

web.insert_cue Create new ToDO note

=TITLE 
    type    char    128
    required
    
=CATEGORY
    type    link    TODO_CATS   NAME
    web.combo
    web.search
    required

=PRIORITY
    type    link    TODO_PRIO   NAME
    web.combo
    required

=DUE_DATE
    type    date
    web.details 1
    web.grid.details 2

=DONE
    type    bool
    web.grid.editable
    web.view.editable

=NOTE
    type    char    4000
    web.rows           5
    web.grid.maxlen
    web.mono

=CTIME
    label   Create time
    type    utime
    read-only
    web.details 2
    web.grid.hide

=MTIME
    label   Last modify time
    type    utime
    read-only
    web.details 2
